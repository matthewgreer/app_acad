<h1>SIGN IN</h1>

<form action="<%= session_url %>" method="POST">

  <input type='hidden'
    name='authenticity_token'
    value='<%= form_authenticity_token %>'>


  <label for="user_email">EMAIL</label>
  <input type="text" 
    name="user[email]" 
    id="user_email"
    >

  <br>
  
  <label for="user[password]">PASSWORD</label>
  <input type="password"
    name="user[password]"
    id="user_password"
   >

  <br>

  <input type="submit" value="SIGN IN">

</form>